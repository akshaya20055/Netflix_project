    "from collections import Counter\n",
    "\n",
    "# Combine all countries into a single list\n",
    "all_countries = ','.join(df['country']).split(',')\n",
    "\n",
    "# Remove spaces\n",
    "all_countries = [country.strip() for country in all_countries if country.strip() != '']\n",
    "\n",
    "# Count frequency\n",
    "country_counts = Counter(all_countries)\n",
    "\n",
    "# Convert to DataFrame\n",
    "country_df = pd.DataFrame(country_counts.items(), columns=['Country', 'Count'])\n",
    "country_df.sort_values(by='Count', ascending=False, inplace=True)\n",
    "\n",
    "# Show top 10 countries\n",
    "country_df.head(10)\n"
   ]
  },
