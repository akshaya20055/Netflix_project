   "source": [
    "from collections import Counter\n",
    "\n",
    "# Combine all genres from the 'listed_in' column\n",
    "all_genres = ','.join(df['listed_in']).split(',')\n",
    "\n",
    "# Remove extra spaces and clean\n",
    "all_genres = [genre.strip() for genre in all_genres]\n",
    "\n",
    "# Count frequency of each genre\n",
    "genre_counts = Counter(all_genres)\n",
    "\n",
    "# Convert to DataFrame for easier plotting\n",
    "genre_df = pd.DataFrame(genre_counts.items(), columns=['Genre', 'Count'])\n",
    "genre_df.sort_values(by='Count', ascending=False, inplace=True)\n",
    "\n",
    "# Show top 10 genres\n",
    "genre_df.head(10)\n"
   ]
  },
